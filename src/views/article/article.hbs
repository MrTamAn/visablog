<div class=''>
  <h1>{{titlePage}}</h1>

  {{#each articles}}
    <div class='card card-item' style='width: 18rem;'>
      <img src={{this.image}} class='card-img-top' alt='...' />
      <div class='card-body'>
        <h5 class='card-title'>{{this.title}}</h5>
        <p class='card-text'>{{this.content}}</p>
        <a href='#' class='btn btn-primary'>Go somewhere</a>
      </div>
    </div>
  {{/each}}
</div>